{{outlet}}
<div class="panel panel-success">
  <div class="panel-heading">
    <h3 class="panel-title">Liga</h3>
  </div>
<form class="form-horizontal">
  <fieldset>
    <div class="form-group">
      <label for="Nombre de liga" class="col-lg-2 control-label">LeagueName</label>
      <div class="col-lg-10">
        <input class="form-control" id="inputLeagueName" placeholder="League" type="text">
      </div>
    </div>
    <div class="form-group">
      <label for="Descripcion" class="col-lg-2 control-label">Descripcion</label>
      <div class="col-lg-10">
        <textarea class="form-control" rows="3" id="textAreaDescription"></textarea>
      </div>
    </div>
    <div class="form-group">
      <label for="Categoria" class="col-lg-2 control-label">Category</label>
      <div class="col-lg-10">
        <input class="form-control" id="inputCategory" placeholder="Category" type="text">
      </div>
    </div>
    <div class="form-group">
  <label for="select" class="col-lg-2 control-label">Ciudad</label>
  <div class="col-lg-10">
    <select class="form-control" id="city">
      <option>Apodaca</option>
      <option>Escobedo</option>
      <option>Monterrey</option>
      <option>San Pedro</option>
      <option>Guadalupe</option>
    </select>
    <br>
  </div>
</div>
  </fieldset>
</form>
<div class="panel-body">
</div>
</div>
