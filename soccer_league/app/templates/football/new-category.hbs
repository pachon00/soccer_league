{{outlet}}
<div class="panel panel-success">
  <div class="panel-heading">
    <h3 class="panel-title">Categoria</h3>
  </div>
<form class="form-horizontal">
  <fieldset>
    <div class="form-group">
      <label for="Nombre de categoria" class="col-lg-2 control-label">CategoryName</label>
      <div class="col-lg-10">
        <input class="form-control" id="inputCategoryName" placeholder="Category" type="text">
      </div>
    </div>
    <div class="input-group">
      <label for="select" class="col-lg-2 control-label">Edad maxima</label><br>
      <input value="1" type="text" class="form-control" width="3" age-max>
      <div class="input-group-btn">
        <button class="btn btn-default" type="button">+</button>
        <button class="btn btn-default" type="button">-</button>
      </div>
    </div>
    <div class="input-group">
      <label for="select" class="col-lg-2 control-label">Edad minima</label><br>
      <input value="1" type="text" class="form-control" width="3" age-min>
      <div class="input-group-btn">
        <button class="btn btn-default" type="button">+</button>
        <button class="btn btn-default" type="button">-</button>
      </div>
    </div>
    <div class="form-group">
      <label for="Descripcion" class="col-lg-2 control-label">Descripcion</label>
      <div class="col-lg-10">
        <textarea class="form-control" rows="3" id="textAreaDescription"></textarea>
      </div>
    </div>
  <div class="form-group">
    <label for="select" class="col-lg-2 control-label">Liga</label>
    <div class="col-lg-10">
      <select class="form-control" id="city">
        <option>Apodaca</option>
        <option>Escobedo</option>
        <option>Monterrey</option>
        <option>San Pedro</option>
        <option>Guadalupe</option>
      </select>
      <br>
    </div>
  </div>
  </fieldset>
</form>
<div class="panel-body">
</div>
</div>
